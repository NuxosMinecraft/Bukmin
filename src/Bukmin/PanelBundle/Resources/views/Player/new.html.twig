{% extends "BukminPanelBundle:Player:base.html.twig" %}

{% block bukminpanel_body %}
  <h1>Player creation</h1>

  <form action="{{ path('player_create') }}" method="post" {{ form_enctype(form) }}>
      {{ form_widget(form) }}
      <p>
	  <button type="submit">Create</button>
      </p>
  </form>

  <ul class="record_actions">
      <li>
	  <a href="{{ path('player') }}">
	      Back to the list
	  </a>
      </li>
  </ul>
{% endblock %}